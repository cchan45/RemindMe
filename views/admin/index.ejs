<table class="table col-md-4">
    <thead>
        <tr>
            <th>Session ID</th>
            <th>User ID</th>
            <th>Revoke</th>
        </tr>
    <tbody>
        <% for (const [sessionId, session] of Object.entries(sessions)) { %>
            <tr>
                <td>
                    <%= sessionId %>
                </td>
                <td>
                    <%= session.passport.user %>
                </td>
                <td>
                    <a class="btn btn-primary" href="<%= "/admin/revoke/" + sessionId %>">Revoke</a>
                </td>
            </tr>
        <% } %>
    </tbody>
</table>

